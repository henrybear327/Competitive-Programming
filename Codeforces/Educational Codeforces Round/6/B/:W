#include <bits/stdc++.h>
//LLONG_MIN LLONG_MAX INT_MIN INT_MAX

#ifdef _WIN32
#define lld "I64d"
#else
#define lld "lld"
#endif

using namespace std;

int num[10] = {6, 2, 5, 5, 4, 5, 6, 3, 7, 6};

int main()
{
    int a, b;
    scanf("%d %d", &a, &b);
    
    int ans = 0;
    for(int i = a; i <= b; i++) {
	ans += num[i];
    }

    printf("%d\n", ans);

    return 0;
}
